{
  "arrowParens": "always",
  "bracketSameLine": false,
  "parser": "typescript",
  "printWidth": 120,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.css",
      "options": { "parser": "css" }
    },

    {
      "files": "*.json",
      "options": { "parser": "json" }
    },
    {
      "files": "*.scss",
      "options": { "parser": "scss" }
    },
    {
      "files": "*.tsh",
      "options": { "parser": "typescript" }
    }
  ]
}
